{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "InOrbit Docs",
  "colors": {
    "primary": "#2A3C98",
    "light": "#3F93FF",
    "dark": "#282967"
  },
  "description": "Documentation for InOrbit's robot operations platform",
  "fonts": {
    "heading": {
      "family": "Manrope"
    }
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "favicon": {
    "light": "/images/brand/favicon.png",
    "dark": "/images/brand/favicon.png"
  },
  "head": [
    "<link rel=\"stylesheet\" href=\"/style.css\">"
  ],
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "navigation": {
    "products": [
      {
        "product": "Home",
        "icon": "house",
        "pages": [
          "index"
        ]
      },
      {
        "product": "Space Intelligence",
        "icon": "rocket",
        "description": "Fleet management and business execution for end users",
        "tabs": [
          {
            "tab": "Overview",
            "pages": [
              "space-intelligence/index"
            ]
          }
        ]
      },
      {
        "product": "Ground Control",
        "icon": "code",
        "description": "Developer tools and SDKs for robot integration",
        "tabs": [
          {
            "tab": "Overview",
            "pages": [
              "ground-control/index"
            ]
          }
        ]
      },
      {
        "product": "OpenRobOps",
        "icon": "code-branch",
        "description": "Open source fleet management platform",
        "pages": [
          "openrobops/index"
        ]
      },
      {
        "product": "Connect",
        "icon": "network-wired",
        "description": "Ecosystem of hardware partners and integrations",
        "pages": [
          "connect/index"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@inorbit.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://console.inorbit.ai"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/inorbitai",
      "github": "https://github.com/inorbit-ai",
      "linkedin": "https://linkedin.com/company/inorbit"
    }
  }
}
